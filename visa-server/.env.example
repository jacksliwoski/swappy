# Server Configuration
PORT=7002
CORS_ORIGIN=http://localhost:5173

# Visa API Configuration
VISA_BASE_URL=https://sandbox.api.visa.com
VISA_USERNAME=your_visa_username_here
VISA_PASSWORD=your_visa_password_here

# Visa mTLS Certificates (paths to .pem files)
VISA_CLIENT_CRT=path/to/your/cert.pem
VISA_CLIENT_KEY=path/to/your/key.pem
VISA_ROOT_CA=path/to/your/ca.pem

# Visa API Parameters
VISA_ACQUIRING_BIN=your_acquiring_bin
VISA_ACQUIRER_COUNTRY_CODE=840
VISA_PAV_PATH=/paymentaccountvalidation/v5/accountverification

# Mock Mode (set to 'false' to use real Visa API)
MOCK=true
